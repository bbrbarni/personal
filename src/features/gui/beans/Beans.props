# list of standard toolbars (containing bean tools that do not wrap weka
# base class types)
features.gui.beans.KnowledgeFlow.standardToolBars=\
 Evaluation,\
 Visualization

# Specifies the tools for each standard toolbar
features.gui.beans.KnowledgeFlow.Evaluation=features.gui.beans.TrainingSetMaker,\
 features.gui.beans.TestSetMaker,\
 features.gui.beans.CrossValidationFoldMaker,\
 features.gui.beans.TrainTestSplitMaker,\
 features.gui.beans.InstanceStreamToBatchMaker,\
 features.gui.beans.ClassAssigner,\
 features.gui.beans.ClassValuePicker,\
 features.gui.beans.ClassifierPerformanceEvaluator,\
 features.gui.beans.IncrementalClassifierEvaluator,\
 features.gui.beans.ClustererPerformanceEvaluator,\
 features.gui.beans.PredictionAppender,\
 features.gui.beans.SerializedModelSaver
features.gui.beans.KnowledgeFlow.Visualization=features.gui.beans.DataVisualizer,\
 features.gui.beans.ScatterPlotMatrix,\
 features.gui.beans.AttributeSummarizer,\
 features.gui.beans.ModelPerformanceChart,\
 features.gui.beans.CostBenefitAnalysis,\
 features.gui.beans.TextViewer,\
 features.gui.beans.GraphViewer,\
 features.gui.beans.StripChart

# the bean panel processes keys in the generic object editor properties file.
# If there is a corresponding entry here, then a toolbar is constructed using
# the specified wrapper bean
features.core.converters.Loader=features.gui.beans.Loader
features.core.converters.Saver=features.gui.beans.Saver
features.classifiers.Classifier=features.gui.beans.Classifier
features.filters.Filter=features.gui.beans.Filter
features.clusterers.Clusterer=features.gui.beans.Clusterer
features.associations.Associator=features.gui.beans.Associator
# features.attributeSelection.ASEvaluation=features.gui.beans.AttributeSelector

# toolbar ordering information for wrapper types
features.core.converters.Loader.order=0
features.core.converters.Saver.order=1
features.filters.Filter.order=3
features.classifiers.Classifier.order=4
features.clusterers.Clusterer.order=5
features.associations.Associator.order=6
# features.attributeSelection.ASEvaluation.order=6

# toolbar naming aliases for weka algorithm classes
features.core.converters.Loader.alias=DataSources
features.core.converters.Saver.alias=DataSinks
features.classifiers.Classifier.alias=Classifiers
features.clusterers.Clusterer.alias=Clusterers
features.filters.Filter.alias=Filters
features.associations.Associator.alias=Associations

# GUI behaviour
ScrollBarIncrementLayout=20
ScrollBarIncrementComponents=50
FlowWidth=1024
FlowHeight=768
PreferredExtension=.kf
UserComponentsInXML=false

# colours
features.gui.beans.StripChart.backgroundColour=black
features.gui.beans.StripChart$LegendPanel.borderColour=blue
#features.gui.beans.StripChart.backgroundColour=white
#features.gui.beans.StripChart$LegendPanel.borderColour=black
